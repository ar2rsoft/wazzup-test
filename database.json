{
    "defaultEnv": "pg",
    "pg": {
        "driver": "pg",
        "user": {
            "ENV": "PG_USER"
        },
        "password": {
            "ENV": "PG_PASSWORD"
        },
        "host": {
            "ENV": "PG_HOST"
        },
        "database": {
            "ENV": "PG_DBNAME"
        },
        "port": {
            "ENV": "PG_PORT"
        },
        "ssl": false,
        "schema": {
            "ENV": "PG_SCHEMA"
        }
    }
}
